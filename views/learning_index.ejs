<h2>Mastery Series</h2><% function gate(track){ if(user.tier==='Elite') return true; if(user.tier==='Pro') return (track!=='advanced'); if(user.tier==='Basic') return (track==='beginner'); return false; } %><div class='grid cols-3'><% tracks.forEach(tr=>{ const pct = (trackProgress[tr.id]||0); %><div class='card <%= gate(tr.id)?'':'small' %>'><div class='row' style='justify-content:space-between;align-items:center'><h3><%= tr.name %></h3><div class='ring' style='--pct:<%= Math.round(pct) %>'><span><%= Math.round(pct) %>%</span></div></div><ul><% modules.filter(m=>m.track===tr.id).forEach(m=>{ const mp = Math.round(moduleProgress[m.id]||0); const done = mp>=100; %><li style='margin:8px 0'><strong>M<%= m.num %>:</strong> <%= m.title %> <% if(gate(tr.id)){ %>— <a href='/learning/module/<%= m.id %>'>Open</a> <% } else { %>— <span class='small'>Upgrade to access</span> <% } %> <%= done?'<span class="badge-complete">Completed</span>':'' %></li><% }) %></ul></div><% }) %></div>