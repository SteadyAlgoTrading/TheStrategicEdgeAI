<div class='row' style='align-items:center;justify-content:space-between'><h2>Projects</h2><a class='btn gold' href='/projects/new'>New Project</a></div><table class='table'><tr><th>Name</th><th>Template</th><th>Status</th><th>Updated</th></tr><% projects.forEach(p=>{ %><tr><td><a href='/projects/<%= p.id %>'><%= p.name %></a></td><td><%= p.template || 'Custom' %></td><td><span class='small'><%= p.status %></span></td><td><%= new Date(p.updatedAt).toLocaleString() %></td></tr><% }) %></table>